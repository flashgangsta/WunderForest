package com.rr {		/**	 * ...	 * @author Sergey Krivtsov (flashgangsta@gmail.com)	 */		public class PersonageModelData extends ModelData {						/**		 * Конструктор		 * @param	data базовые данные модиели персонажа		 */				public function PersonageModelData( data:Object ) {			super();			super.data = data;		}				/**		 * Цвет персонажа		 */				public function get color():uint {			return data.color;		}				/**		 * Стандартное количество жизни персонажа		 */				public function get hp():int {			return data.hp;		}				/**		 * Стандартное количество настроения персонажа		 */				public function get mood():int {			return data.mood;		}				/**		 * Стандартное количество нервов персонажа		 */				public function get nerve():int {			return data.nerve;		}				/**		 * Стандартное количество сытости персонажа		 */				public function get satiety():int {			return data.satiety;		}		 		/**		 * Стандартное количество защиты персонажа		 */				public function get defence():int {			return data.defence;		}				/**		 * Является ли персонаж новым (для отображения иконки в магазине персонажей)		 */				public final function get isNew():Boolean {			return Boolean( data.isNew );		}	}}