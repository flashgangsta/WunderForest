package com.rr {		/**	 * ...	 * @author Sergey Krivtsov (flashgangsta@gmail.com)	 */		public class ModelData {				/// Данные с сервера		protected var data:Object = {};				/**		 * Конструктор		 */				public function ModelData() {					}				/**		 * Идентификатор модели		 */				public final function get id():int {			return data.id;		}				/**		 * Ссылка на файл		 */				public final function get path():String {			return data.path;		}				/**		 * Ссылка на файл превью		 */				public final function get previewPath():String {			var result:String = data.previewPath;			return result && result.length ? result : data.path;		}				/**		 * Цена		 */				public final function get price():Number {			return data.price;		}				/**		 * Цена разблокировки		 */				public final function get unlockPrice():Number {			return data.unlockPrice;		}				/**		 * Минимальный уровень требуемый для покупки		 */				public final function get minLvl():int {			return data.minLvl;		}				/**		 * Описание		 */				public final function get description():String {			return data.description;		}						/**		 * Стандартное имя		 */				public function get name():String {			return data.name;		}				/**		 * Ссылка на объект с сервера		 */				public function getData():Object {			return data;		}			}}