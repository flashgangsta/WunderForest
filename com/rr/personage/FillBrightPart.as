package  com.rr.personage {		import flash.display.Shape;	import fl.motion.AdjustColor;	import flash.display.Sprite;	import flash.geom.ColorTransform;	import flash.filters.ColorMatrixFilter;			public class FillBrightPart extends ColorablePart {						public function FillBrightPart() {			if( root ) setColor();		}				override public function setColor():void {			var colorTrans = new ColorTransform();			var shape:Shape = getChildAt( 0 ) as Shape;			var colorFilter:AdjustColor;			var colorMatrix:ColorMatrixFilter;			var color:uint = getColor();						if ( shape.transform.colorTransform.color === color ) {				return;			}						colorTrans.color = color;			shape.transform.colorTransform = colorTrans;			colorFilter = new AdjustColor();			colorFilter.brightness = 4;//-6;			colorFilter.contrast = -16;//-25;			colorFilter.saturation = 100;//-27;			colorFilter.hue = -9;//6;			colorMatrix = new ColorMatrixFilter( colorFilter.CalculateFinalFlatArray() );			filters = [ colorMatrix ];		}	}	}