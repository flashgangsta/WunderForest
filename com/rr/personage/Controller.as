package com.rr.personage {		import flash.display.MovieClip;	import flash.display.Sprite;		/*	 * ...	 * @author Sergey Krivtsov	 */		public class Controller extends Sprite {				private var personage:Personage;						/**		 * 		 */				public function Controller() {			personage = new Personage( 0x00CCFF, Personage.FEMALE );			addChild( personage.showPose( Personage.WITHOUT_GUN ) );		}				/**		 * 		 * @return		 */				public function getPose():MovieClip {			return personage.getPose();		}			}}