package  com.rr.personage {		import flash.display.Sprite;	import flash.filters.ColorMatrixFilter;	import fl.motion.AdjustColor;	import flash.geom.ColorTransform;		public class FillPart extends ColorablePart {				private var colorTrans = new ColorTransform();				public function FillPart() {			if( root ) setColor();		}				override public function setColor():void {			var color:uint = getColor();			if ( transform.colorTransform.color === color ) {				return;			}			colorTrans.color = color;			transform.colorTransform = colorTrans;		}	}	}