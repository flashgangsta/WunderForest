package com.rr.pages.cellar {		import com.rr.ItemData;	import com.rr.ui.ItemWithIcon;	import com.rr.utils.InteractiveKiller;			/**	 * ...	 * @author Sergey Krivtsov	 */		public dynamic class CellarItem extends ItemWithIcon {				/// Данные о предмете		private var data:ItemData;				/**		 * Конструктор		 */				public function CellarItem() {			visible = false;			InteractiveKiller.kill( icon );		}				/**		 * Выводит данные о предмете		 * @param	data		 */				public function fill( data:ItemData ):void {			visible = true;			this.data = data;			loadIcon( data.previewPath );		}				/**		 * Очищает данные		 */				public function clear():void {			if ( !visible ) return;			visible = false;			removeIcon();			data = null;		}				public function getItemData():ItemData {			return data;		}	}	}