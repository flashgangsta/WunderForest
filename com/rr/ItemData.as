package com.rr {	/**	 * ...	 * @author Sergey Krivtsov (flashgangsta@gmail.com)	 */	public class ItemData extends ItemModelData {				static public const SIDE_LEFT:String = "left";		static public const SIDE_RIGHT:String = "right";				/// данные о предмете с сервера		private var itemData:Object;				/**		 * Конструктор		 * @param	data данные о предмете с сервера		 */				public function ItemData( data:Object ) {			itemData = data;			var itemModelData:ItemModelData = Helper.getItemModels().getModelByID( itemData.id ) as ItemModelData;			super( itemModelData.getData() );		}				/**		 * Уникальный идентификатор конкретного предмета		 */				public function get iid():int {			return itemData.iid;		}				/**		 * Возвращает сторону на которую одет предмет (если он имеет ее)		 */				public function get side():String {			if ( !itemData.side ) throw new Error( "Запрошена сторона у предмета не имеющего значения параметра side" );			return itemData.side === 1 ? SIDE_LEFT : ( itemData.side === 2 ? SIDE_RIGHT : null );		}			}}