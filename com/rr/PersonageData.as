package com.rr {	/**	 * ...	 * @author Sergey Krivtsov (flashgangsta@gmail.com)	 */	public class PersonageData extends PersonageModelData {				/// Данные персонажа		private var personageData:Object;				/**		 * Конструктор		 * @param	data		 */				public function PersonageData( data:Object ) {				personageData = data;			var modelData:PersonageModelData = Helper.getPersonageModels().getModelByID( personageData.id ) as PersonageModelData;			super( modelData );		}						/**		 * Возвразает имя персонажая присвоенное игроком		 */				override public function get name():String {			return personageData.name;		}				/**		 * 		 */				override public function get color():uint {			return personageData.color;		}				/**		 * Возвращает настоящий уровень игрока		 */				public function get level():int {			return personageData.lvl;		}				/**		 * Уникальный идентификатор конкретного персонажа		 */				public function get pid():int {			return personageData.pid;		}				/**		 * 		 */				public function get wins():int {			return personageData.wins;		}				/**		 * 		 */				public function get loss():int {			return personageData.loss;		}				/**		 * Общее количество боев		 */				public function get battlesCount():int {			return wins + loss;		}				/**		 * Возвразает список iid предметов надетых на персонажа		 */				public function get equipmentList():Array {			return personageData.equipment;		}				/**		 * 		 */				public function get sex():int {			return personageData.sex;		}			}}